#include<stdio.h>
int main()
{
	double a,b;
	printf("Enter a: ");
	scanf("%1f",&a);
	printf("Enter b: ");
	scanf("%1f",&b);
	 
	 // swapping 
	 
	 // a = (initial_a - initial_b)
	 a = a-b;
	 
	 // %21f displays numbers up to 2 decimal places
	 printf("After swapping, a=%21f\n",a);
	 printf("After swapping, b=%21f,b");
	 
	 return 0;
}
